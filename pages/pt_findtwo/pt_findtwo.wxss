/* pages/pt_findtwo/pt_findtwo.wxss */
page{
  background: #f2f2f2;  
}
.choose{
  background: #fff;
  display: flex;
  justify-content: space-around;
  color: #333;
  font-size: 32rpx;
  padding-bottom: 10rpx;
  box-sizing: border-box;
}
.choose image{
  width: 23rpx;
  height: 13rpx;
  vertical-align:middle;
}
.adres{
  display: inline-block;
  margin-right: 15rpx;
  vertical-align: middle;
}
.type{
  display: inline-block;
  margin-right: 15rpx;
}
.kongtu{
  position: absolute;
  left: 50%;
  top: 30%;
  transform: translate(-50%,-50%);
  text-align: center;
  font-size: 28rpx;
  color: #333;
}
.kongtu>image{
  width: 180rpx;
  height: 180rpx;
}
.zi{
  margin: 40rpx 0;
}
.enter{
  width: 250rpx;
  height: 68rpx;
  text-align: center;
  line-height: 68rpx;
  background: #fe5ba9;
  color: #fff;
  margin-left: 58rpx;
}
.l-inst{
  background-color: #ffffff;

}
.l-inst-t{
  border-top: 1rpx #f2f2f2 solid;
  padding: 25rpx;
  box-sizing: border-box;
  font-size: 22rpx;
}
.timg{
  width: 206rpx;
  height: 160rpx;
  float: left;
  margin-right: 20rpx;
}
.l-gsm{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 22rpx;
}
.timg image{
  width: 100%;
  height: 100%; 
}
.l-renzhen{
   font-size: 22rpx;
